  "dependencies": {
    "@babel/polyfill": "^7.0.0-rc.3",
    "app-root-path": "^2.1.0",
    "chewit": "^1.0.10",
    "choo": "^6.13.0",
    "choo-async": "^0.1.1",
    "choo-devtools": "^2.5.1",
    "choo-ssr": "^0.2.1",
    "colorette": "^1.0.1",
    "fastify": "^1.10.0",
    "fastify-compress": "^0.6.0",
    "fastify-helmet": "^2.1.0",
    "fastify-static": "^0.14.0",
    "graceful-fs": "^4.1.11",
    "nanocomponent": "^6.5.2",
    "xtend": "^4.0.1"
  },
  "description": "Offical website of Ideas Never Cease",
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.3",
    "@babel/core": "^7.0.0-rc.3",
    "@babel/plugin-external-helpers": "7.0.0-rc.3",
    "@babel/plugin-proposal-class-properties": "7.0.0-rc.3",
    "@babel/plugin-proposal-decorators": "7.0.0-rc.3",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0-rc.3",
    "@babel/plugin-proposal-function-sent": "7.0.0-rc.3",
    "@babel/plugin-proposal-json-strings": "7.0.0-rc.3",
    "@babel/plugin-proposal-numeric-separator": "7.0.0-rc.3",
    "@babel/plugin-proposal-throw-expressions": "7.0.0-rc.3",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-rc.3",
    "@babel/plugin-syntax-import-meta": "7.0.0-rc.3",
    "@babel/preset-env": "^7.0.0-rc.3",
    "@babel/register": "^7.0.0-rc.3",
    "babel-loader": "^7.1.5",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.65.0",
    "rollup-plugin-babel": "^4.0.0-beta.8",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "sass": "^1.13.0",
    "snazzy": "^7.1.1",
    "standardx": "^2.1.0",
    "updates": "^4.2.1"
  },
  "homepage": "https://inc.sh",
  "main": "server.js",
  "name": "!NC",
  "private": true,
  "scripts": {
    "build": "rollup --config && npm run css",
    "css": "sass --update app/sass:app/dist --style compressed",
    "deploy": "vagrant push",
    "start": "npm run build && NODE_ENV=production node server",
    "test": "run-s test:*",
    "test:dependencies": "updates --update ./ --exclude rollup-plugin-babel",
    "test:lint": "standardx --verbose | snazzy",
    "watch": "run-p watch:*",
    "watch:development": "rollup --config --watch",
    "watch:sass": "sass --watch app/sass:app/dist --style compressed",
    "watch:server": "NODE_ENV=development nodemon server"
  },
  "standardx": {
    "ignore": [
      "app/dist"
    ]
  },
  "version": "2.0.0"
}
